const mongoose = require("mongoose");


const frontUsers = new mongoose.Schema({
    //前台用户的姓名
    front_name: String,
    //前台用户的年龄
    front_age: String,
    //前台用户的性别
    front_gender: String,
    //前台用户的密码
    front_password: String,
    //前台用户的电话
    front_phone: String,
})


mongoose.model("frontUsersModel", frontUsers, "frontUsers");